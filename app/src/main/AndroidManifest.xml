<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.sicredi.sicredieventos">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SicrediEventos">
        <activity android:name="br.com.sicredi.sicrediEventos.DadosDoEventoView"></activity>
        <activity android:name="br.com.sicredi.sicrediEventos.eventos.view.EventosView" />
        <activity
            android:name="br.com.sicredi.sicrediEventos.TelaInicialDeCarregamento"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar"
            tools:node="merge">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>